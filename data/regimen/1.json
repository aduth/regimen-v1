{
  "week": 2,
  "fields": {
    "bp_test_weight": 255,
    "bp_test_interval": 3,
    "bp_1rm": 238,
    "bp_5rm" : 212
  },
  "program": {
    "name": "Madcow 5x5",
    "description": "Originally created by Bill Starr, the writer of the book “The Strongest Shall Survive” this program is great for adding muscle mass and increasing overall strength and fitness levels.",
    "fields": [
      {
        "type": "test",
        "name": "bp",
        "label": "Bench Press"
      },
      {
        "type": "number",
        "name": "pr_week",
        "label": "Match PRs in Week #",
        "description": "The week in which you want to match your previous 5RMs"
      },
      {
        "type": "number",
        "name": "interval",
        "label": "Set Interval",
        "description": "The size of the steps between your ramped sets"
      }
    ],
    "iteration": {
      "type": "week",
      "label": "Week {{week}}"
    },
    "groups": [
      {
        "name": "Monday",
        "exercises": [
          {
            "label": "Bench Press",
            "interval_type": "rep",
            "sets": [
              {
                "interval": 5,
                "weight": "{{set5}}*(1-({{interval*4}}/100))"
              },
              {
                "interval": 5,
                "weight": "{{set5}}*(1-({{interval*3}}/100))"
              },
              {
                "interval": 5,
                "weight": "{{set5}}*(1-({{interval*2}}/100))"
              },
              {
                "interval": 5,
                "weight": "{{set5}}*(1-({{interval}}/100))"
              },
              {
                "interval": 5,
                "weight": "{{week}}*({{bp_5rm}}*(1/1.025)^({{pr_week}}-1))"
              }
            ],
            "text": "{{set.interval}} reps at {{set.weight}} {{unit}}"
          }
        ]
      }
    ]
  }
}
